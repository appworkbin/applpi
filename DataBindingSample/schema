CREATE SEQUENCE acs_user_seq;CREATE SEQUENCE acs_role_seq;
CREATE TABLE acs_user (    id NUMBER DEFAULT acs_user_seq.NEXTVAL NOT NULL,    referenceId VARCHAR2(255) NOT NULL,    username VARCHAR2(100) NOT NULL,    password VARCHAR2(200) NOT NULL,    enabled NUMBER(1) DEFAULT 1 NOT NULL,    credentialsexpired NUMBER(1) DEFAULT 0 NOT NULL,    expired NUMBER(1) DEFAULT 0 NOT NULL,    locked NUMBER(1) DEFAULT 0 NOT NULL,    version NUMBER(1) NOT NULL,    createdBy VARCHAR2(100) NOT NULL,    createdAt TIMESTAMP NOT NULL,    updatedBy VARCHAR2(100) DEFAULT NULL,    updatedAt TIMESTAMP DEFAULT NULL,    PRIMARY KEY (id),    CONSTRAINT UQ_Account_ReferenceId UNIQUE (referenceId),    CONSTRAINT UQ_Account_Username UNIQUE (username) ); 
CREATE TABLE acs_role (    id NUMBER DEFAULT acs_role_seq.NEXTVAL NOT NULL,    code VARCHAR2(50) NOT NULL,    label VARCHAR2(100) NOT NULL,    ordinal NUMBER(1) NOT NULL,    effectiveAt TIMESTAMP NOT NULL,    expiresAt TIMESTAMP DEFAULT NULL,    createdAt TIMESTAMP NOT NULL,    PRIMARY KEY (id),    CONSTRAINT UQ_Role_Code UNIQUE (code) ); 
CREATE TABLE acs_user_role (    accountId NUMBER NOT NULL,    roleId NUMBER NOT NULL,    PRIMARY KEY (accountId, roleId),    CONSTRAINT FK_AccountRole_AccountId FOREIGN KEY (accountId) REFERENCES acs_user (id),    CONSTRAINT FK_AccountRole_RoleId FOREIGN KEY (roleId) REFERENCES acs_role (id) ); 
